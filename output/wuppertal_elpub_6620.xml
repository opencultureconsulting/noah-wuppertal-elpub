                    <titleInfo lang="ger">
                        <title>Entwicklung unternehmerischer Persönlichkeiten bei Existenzgründungen aus der Arbeitslosigkeit</title>
                    </titleInfo>
                    <name type="personal">
                        <displayForm>Oberländer, Anja</displayForm>
                        <namePart type="family">Oberländer</namePart>
