                    <titleInfo lang="ger">
                        <title>Planung von ländlichen Niederspannungsnetzen mit innovativen Lösungsoptionen</title>
                    </titleInfo>
                    <titleInfo lang="eng" type="translated">
                        <title>Planning of rural low voltage networks considering innovative technologies</title>
                    </titleInfo>
