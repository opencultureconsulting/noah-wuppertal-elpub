                    <titleInfo lang="ger">
                        <title>Optimale Ansteuerung von Mittelspannungswechselrichtern</title>
                    </titleInfo>
                    <name type="personal">
                        <displayForm>Boller, Till</displayForm>
                        <namePart type="family">Boller</namePart>
                        <namePart type="given">Till</namePart>
                        <role>
