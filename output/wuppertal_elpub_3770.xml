                    <titleInfo lang="ger">
                        <title>Computersimulationen von Silica-Silan-Polymer Grenzflächen</title>
                    </titleInfo>
                    <name type="personal">
                        <displayForm>Hojdis, Nils Walter</displayForm>
                        <namePart type="family">Hojdis</namePart>
                        <namePart type="given">Nils Walter</namePart>
