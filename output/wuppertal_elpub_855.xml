                    <titleInfo lang="ger">
                        <title>Noris Konstruktion und Bahnenabschlüsse in der Varietät der Algebren</title>
                    </titleInfo>
                    <name type="personal">
                        <displayForm>Olbricht, Roland</displayForm>
