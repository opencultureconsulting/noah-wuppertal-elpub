                    <titleInfo lang="eng">
                        <title>Search for a heavy Scalar Boson in the 2 Higgs Doublet Model with the ATLAS Detector</title>
                    </titleInfo>
                    <name type="personal">
                        <displayForm>Ernis, Gunar</displayForm>
                        <namePart type="family">Ernis</namePart>
                        <namePart type="given">Gunar</namePart>
