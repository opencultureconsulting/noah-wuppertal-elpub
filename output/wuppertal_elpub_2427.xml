                    <titleInfo lang="ger">
                        <title>Analytik von Arzneimittelwirkstoffen und Untersuchung ihres Verhaltens während der Trinkwasseraufbereitung mit Ozon</title>
                    </titleInfo>
                    <titleInfo lang="eng" type="translated">
                        <title>Analysis of pharmaceuticals and their behaviour during drinking water treatment with ozone</title>
                    </titleInfo>
                    <name type="personal">
                        <displayForm>Preuß, Bibiana</displayForm>
                        <namePart type="family">Preuß</namePart>
                        <namePart type="given">Bibiana</namePart>
                        <role>
                            <roleTerm type="code" authority="marcrelator">aut</roleTerm>
                        </role>
                    </name>
                    <typeOfResource>text</typeOfResource>
